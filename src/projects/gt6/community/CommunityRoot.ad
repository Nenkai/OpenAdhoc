                        method fire(context, w)
                        {
                            if (self.active_body != nil)
                            {
                                self.active_body.insensitive = true;
                                var o = self.active_body.Opacity;
                                o.reverse = true;
                                o.start();
                            }

                            self.active_body = w;
                            var o = w.Opacity;
                            o.reverse = false;
                            o.start();
                            self.active_body.insensitive = false;
