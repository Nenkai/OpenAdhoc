    function startAdHocMatching()
    {
        var res = false;
        if (sJoinMode)
        {
            res = main::PDINetwork.startAdHocMatching(main::pdistd::AdHocMatchingMode::Client,
                                                      nil,
                                                      5);
        }
        else
        {
            res = main::PDINetwork.startAdHocMatching(main::pdistd::AdHocMatchingMode::Host,
                                                      4,
                                                      5);
        }

        if (!res)
            finalizeNetwork();


        return res;
    }







